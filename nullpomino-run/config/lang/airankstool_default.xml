<!--Main window (AIRanksTool)-->
<entry key="Main_Title">AI Ranks Tool</entry>
<!--Generation Tab -->
<entry key="Main_Generation_Tab_Title">Generation</entry>
<entry key="Main_Input_Label">Input File</entry>
<entry key="Main_Input_Tip">File that will be loaded to iterate upon - leave blank if it is the first time you use the generator</entry>
<entry key="Main_New_File_Text">(New File)</entry>
<entry key="Main_Output_Label">Output File</entry>
<entry key="Main_Output_Tip">File that will be saved after iterations are done</entry>
<entry key="Main_Iterations_Label">Number of iterations</entry>
<entry key="Main_Iterations_Tip">Number of iterations to run over the ranks in input file</entry>
<entry key="Main_Go_Label">Go</entry>
<entry key="Main_Go_Tip">Launch the iterations</entry>
<entry key="Main_Bests_Label">View Bests</entry>
<entry key="Main_Bests_Tip">View the highest ranked surfaces in the ranks in input file</entry>
<entry key="Main_Worsts_Label">View Worsts</entry>
<entry key="Main_Worsts_Tip">View the lowest ranked surfaces in the ranks in input file</entry>

<!--AI Config Tab-->
<entry key="Main_AI_Config_Tab_Title">AI Config</entry>
<entry key="Main_Ranks_File_Used_Label">Ranks File used</entry>
